<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="complain" extends="struts-default,json-default" namespace="/nsfw">
	<action name="complain_*" class="nsfw.complain.action.ComplainAction" method="{1}">
	<result name="{1}">/WEB-INF/jsp/nsfw/complain/{1}.jsp</result>
	<result name="list" type="redirectAction">
	<param name="actionName">complain_listUI</param>
	<param name="startTime">${startTime}</param>
	<param name="endTime">${endTime}</param>
	<param name="complain.toCompTitle">${strTitle}</param>
	<param name="complain.compState">${strState}</param>
	<param name="currentPage">${currentPage}</param>
	<param name="encode">true</param>
	</result>
	<result name="annualStatistic" type="json">
	<param name="root">return_data</param>
	</result>
	</action>
	</package>
</struts>
